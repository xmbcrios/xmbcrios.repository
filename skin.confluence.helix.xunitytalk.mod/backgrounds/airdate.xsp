<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="episodes">
    <name>Sort by airdate</name>
    <match>all</match>
    <rule field="airdate" operator="after">
        <value>2000-01-01</value>
    </rule>
    <order direction="descending">airdate</order>
</smartplaylist>
